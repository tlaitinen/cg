entity Company {
    instance of Versioned, Restricted, Deletable;
    isinCode Text;
    dateFounded Day;
    ipoDate Day;
    delistedDate Day;
}

enum CFName = Name
            | Country
            | Sector
            | Segment
            | ListedSite
            | DualClassShares
            | NominationCommittee;

entity CompanyField {
    instance of Versioned, Deletable, HasDateRange, IsTypedField;
    name CFName;
}
